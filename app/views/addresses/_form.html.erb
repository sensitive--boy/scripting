<%= form_for @address do |f| %>
  <div class="fields">
    <%= f.label :venue %>
    <%= f.text_field :venue %>
  </div>

  <div class="fields">
    <%= f.label :addition %>
    <%= f.text_field :addition %>
  </div>

  <div class="fields">
    <%= f.label :address %>
    <%= f.text_field :address %>
  </div>

  <div class="fields">
    <%= f.label :city %>
    <%= f.text_field :city %>
  </div>

  <div class="fields">
    <%= f.label :country %>
    <%= f.text_field :country %>
  </div>

  <div class="fields">
    <%= f.label :zip %>
    <%= f.text_field :zip %>
  </div>

  <%= f.submit %> | 
<% end %>